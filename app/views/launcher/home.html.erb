<!-- 850 x 750 -->

<style>
img {
  display: block;
  margin-left: auto;
  margin-right: auto
}
</style>

<script>
$(document).ready(function() {
  $("a").click(function() {
    var id = $(this).data("id");
    HelperObject.run(id);
  });
});
</script>

<% if user_signed_in? %>
  <b>Run me as admin!</b>
  <%= link_to (image_tag 'games/blade-and-soul.png', width: 250), '#', data: { id: 'bns' } %>
  <hr/>
  <%= link_to (image_tag 'games/star-wars-the-old-republic.png', width: 250), '#', data: { id: 'swtor' } %>
<% else %>
  <%= render "devise/sessions/form" %>
<% end %>
