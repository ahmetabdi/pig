<div id="containerWrap">
   <div id="header"></div>
   <div id="contents">
      <header id="topHeader">
         <h1 class="subTitle">Sign up</h1>
      </header>
      <div id="contentWrap">
         <div class="wrapLogin">
            <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
               <div class="login-input">
                  <fieldset>
                     <div class="inputGroup">
                        <%= f.label :email, 'Email Address' %>
                        <%= f.email_field :email, placeholder: "Email Address", autofocus: true, autocomplete: false %>
                     </div>
                     <div class="inputGroup wrapPassword">
                        <%= f.label :password, 'Password' %>
                        <%= f.password_field :password, autocomplete: "off", placeholder: "Password" %>
                     </div>
                     <div class="inputGroup wrapPassword">
                        <%= f.label :password_confirmation, 'Password Confirmation' %>
                        <%= f.password_field :password_confirmation, autocomplete: "off", placeholder: "Password Confirmation" %>
                     </div>
                     
                     <%= devise_error_messages! %>

                     <div class="wrapButton">
                        <%= f.submit "Sign up", class: "button squareSingle" %>
                     </div>

                     <div class="wrapLink">
                        <span class="link">
                           <%= link_to "Login", launcher_path, class: "button text" %>
                        </span>
                        <span class="bar"></span>
                        <span class="link">
                           <%= link_to "CanÊ¹t log in?", new_password_path(resource_name), class: "button text" %>
                        </span>
                     </div>
                  </fieldset>
               </div>
            <% end %>
         </div>
      </div>
   </div>
</div>
